       RowBox[{
        RowBox[{"-", "5"}], "\[LessEqual]", 
        RowBox[{"Re", "@", "\[Lambda]"}], "\[LessEqual]", "15"}], ",", 
       RowBox[{
        RowBox[{"-", "15"}], "\[LessEqual]", 
        RowBox[{"Im", "@", "\[Lambda]"}], "\[LessEqual]", "15"}]}], "}"}], 
     "]"}], "//", "Chop"}]}], ";"}]}], "Input",
