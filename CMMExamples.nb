      RowBox[{
       RowBox[{"-", 
        SuperscriptBox["\[FormalLambda]", "2"]}], ",", 
       RowBox[{"1", "-", "\[ImaginaryI]"}]}], "}"}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"{", 
     RowBox[{"1", ",", "0"}], "}"}], "}"}], ",", 
