0f859ac16547"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"sys6b", "[", "L_", "]"}], "=", 
   RowBox[{
    RowBox[{"ToMatrixSystem", "[", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"w", "''''"}], "[", "x", "]"}], "-", 
        RowBox[{
         RowBox[{"w", "''"}], "[", "x", "]"}], "+", 
        RowBox[{"2", 
         RowBox[{"D", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"w0", "[", "x", "]"}], 
            RowBox[{"w", "[", "x", "]"}]}], ",", 
           RowBox[{"{", 
            RowBox[{"x", ",", "2"}], "}"}]}], "]"}]}], "+", 
        RowBox[{"\[Lambda]", " ", 
         RowBox[{"w", "[", "x", "]"}]}]}], "\[Equal]", "0"}], ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"w", "'"}], "[", "0", "]"}], "\[Equal]", "0"}], ",", 
        RowBox[{
         RowBox[{
          RowBox[{"w", "'''"}], "[", "0", "]"}], "\[Equal]", "0"}]}], "}"}], 
      ",", "w", ",", 
      RowBox[{"{", 
       RowBox[{"x", ",", "0", ",", "L"}], "}"}], ",", "\[Lambda]"}], "]"}], "/.", 
    RowBox[{"w0", "\[Rule]", 
     RowBox[{"Function", "[", 
      RowBox[{
       RowBox[{"{", "x", "}"}], ",", 
       RowBox[{
        FractionBox["3", "2"], " ", 
        SuperscriptBox[
         RowBox[{"Sech", "[", 
          FractionBox["x", "2"], "]"}], "2"]}]}], "]"}]}]}]}], ";"}]], "Input",
