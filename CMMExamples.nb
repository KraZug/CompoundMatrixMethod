Cell[BoxData[
 RowBox[{
  RowBox[{"sys6", "[", "L_", "]"}], "=", 
  RowBox[{
   RowBox[{"ToMatrixSystem", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"w", "''''"}], "[", "x", "]"}], "-", 
       RowBox[{
        RowBox[{"w", "''"}], "[", "x", "]"}], "+", 
       RowBox[{"2", 
        RowBox[{"D", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"w0", "[", "x", "]"}], 
           RowBox[{"w", "[", "x", "]"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"x", ",", "2"}], "}"}]}], "]"}]}], "+", 
       RowBox[{"\[Lambda]", " ", 
        RowBox[{"w", "[", "x", "]"}]}]}], "\[Equal]", "0"}], ",", 
     RowBox[{"{", "}"}], ",", "w", ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", 
       RowBox[{"-", "L"}], ",", "L"}], "}"}], ",", "\[Lambda]"}], "]"}], "/.", 
   RowBox[{"w0", "\[Rule]", 
    RowBox[{"Function", "[", 
     RowBox[{
      RowBox[{"{", "x", "}"}], ",", 
      RowBox[{
       FractionBox["3", "2"], " ", 
       SuperscriptBox[
        RowBox[{"Sech", "[", 
         FractionBox["x", "2"], "]"}], "2"]}]}], "]"}]}]}]}]], "Input",
