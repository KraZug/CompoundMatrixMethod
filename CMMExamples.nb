  3.731735849513821*^9, 3.7317358560931973`*^9}, {3.7317359844995418`*^9, 
  3.7317360258699083`*^9}, {3.7468644784230537`*^9, 
  3.746864478981333*^9}},ExpressionUUID->"0db1f408-3627-44bc-b1ac-\
325817a4f849"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"nsol", "=", 
  RowBox[{"NSolve", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"det", "\[Equal]", "0"}], ",", 
      RowBox[{"0.1", "<", 
       RowBox[{"Abs", "[", "\[Sigma]", "]"}], "<", "10"}]}], "}"}], ",", 
    "\[Sigma]"}], "]"}]}]], "Input",
 CellChangeTimes->{3.7317357849091263`*^9},
 CellLabel->
  "In[299]:=",ExpressionUUID->"b6910023-d588-4651-9877-87fdc9ad7743"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"\[Sigma]", "\[Rule]", 
     RowBox[{
      RowBox[{"-", "5.856639172622972`"}], "+", 
      RowBox[{"0.`", " ", "\[ImaginaryI]"}]}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"\[Sigma]", "\[Rule]", 
     RowBox[{
      RowBox[{"-", "1.`"}], "+", 
      RowBox[{"0.`", " ", "\[ImaginaryI]"}]}]}], "}"}], ",", 
